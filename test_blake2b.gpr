with "sparkpass";

project Test_Blake2b is
   for Languages use ("Ada");
   for Source_Dirs use ("test");
   for Object_Dir use "obj/test";
   for Exec_Dir use "bin";
   for Main use ("test_blake2b_vectors.adb", "test_one_byte_zero.adb");

   package Compiler is
      for Default_Switches ("Ada") use ("-O2", "-gnatn", "-gnatwa");
   end Compiler;

   package Linker is
      for Default_Switches ("Ada") use ("-mmacosx-version-min=26.0");
   end Linker;
end Test_Blake2b;
