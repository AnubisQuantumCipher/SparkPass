project Test_Keccak_Debug is
   for Source_Dirs use ("test", "src/sparkpass", "src/sparkpass/crypto");
   for Object_Dir use "obj";
   for Exec_Dir use "bin";
   for Main use ("test_keccak_debug.adb");

   package Compiler is
      for Default_Switches ("Ada") use ("-gnatwa", "-gnat2022", "-O2");
   end Compiler;

   package Linker is
      for Default_Switches ("Ada") use ("-lsodium");
   end Linker;
end Test_Keccak_Debug;
